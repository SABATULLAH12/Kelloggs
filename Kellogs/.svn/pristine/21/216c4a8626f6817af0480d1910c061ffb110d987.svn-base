@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta http-equiv="Cache-control" content="no-cache, no-store, must-revalidate">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/Images/favicon.ico" rel="shortcut icon" type="image/x-icon" />


    @*jquery plugin start*@
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    @*jquery plugin end*@
        
    @*plugin for search dropdown - start*@
    <script src="~/Scripts/Plugins/bootstrap.min.js"></script>
    <link href="~/Scripts/Plugins/bootstrap.min.css" rel="stylesheet" />
    @*plugin for search dropdown - end*@

    <link href="~/Content/Fonts/CustomFonts.css" rel="stylesheet" />
    <link href="~/Content/Shared/Master.css" rel="stylesheet" type="text/css" />   
    
</head>
<body id="parentBody" >
    <content id="parent">
        <div class="default-background_landing" style="display:none"></div>
        <div class="overlay1"></div>
        @*background default start*@
        <div class="default-background"></div>
        @*background default end*@
        @*heade start*@
        <div class="body-header">
            <div class="body-header-menus">
                <div class="header-left">
                    
                </div>
                <div class="header-center">
                </div>
                <div class="header-right">

                    <div class="header-logo-styles">
                        <div class="header-logo"></div>
                    </div>

                    <div class="icons">
                        <div class="home" onclick="location.href='@Url.Action("", "Home")'"></div>
                    </div>
                    @*settings dropdwon div - start*@
                    <div class="settings dropdown disableSelection">
                        
                    </div>
                    @*settings dropdwon div - end*@
                </div>
            </div>
        </div>
        @*header end*@

        @*body start*@
        <div class="body-container">            
            @RenderBody()
        </div>
        @*body end*@
    </content>
    <div id="warning-message">
        this website is only viewable in landscape mode
    </div>
</body>

</html>
