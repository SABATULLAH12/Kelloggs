#samplesize_content > table {
    height: 100%;
    width: 100%;
    /*float: left;*/
}

    #samplesize_content > table > tbody {
        height: 100%;
        width: 100%;
        float: left;
        overflow-x: hidden;
        overflow-y: hidden;
    }

        #samplesize_content > table > tbody > tr {
            /*height: 10%;*/
            width: 100%;
            /*float: left;*/
            font-size: 15px;
            letter-spacing: 0.3px;
            color: #000000;
            display: table;
        }

            #samplesize_content > table > tbody > tr:not(.firstRow) {
                border-bottom-width: 0.07vw;
                border-bottom-color: #BBBBBB;
                border-bottom-style: dashed;
                display: table;
                min-height: 42px;
            }

            #samplesize_content > table > tbody > tr.firstRow {
                font-size: 16px;
                letter-spacing: 0.32px;
                color: #000000;
                text-transform: uppercase;
                padding: 10px 0vw 15px 0vw;
                border-collapse: separate;
            }

.tbl_sampleSize tr td:first-child {
    width: 32%;
    /* float: left; */
    text-align: center;
    position: relative;
}

.tbl_sampleSize tr:not(.firstRow) td:first-child:after {
    display: inline-block;
    width: 0.07vw;
    /*height: 2.2vw;*/
    height: 75%;
    background: grey;
    content: "";
    position: absolute;
    /*top: 0.37vw;*/
    top: 10%;
    right: 0vw;
}

.tbl_sampleSize tr:not(.firstRow) td:nth-child(2):not(:last-child)::after {
    display: inline-block;
    width: 0.07vw;
    /*height: 2.2vw;*/
    height: 75%;
    background: #000000;
    content: "";
    position: absolute;
    /*top: 0.37vw;*/
    top: 10%;
    right: -2px;
}

.tbl_sampleSize tr td:not(:first-child) {
    width: 33%;
    /* float: left; */
    position: relative;
    text-align: center;
}

.tbl_sampleSize tr.firstRow td:first-child::after {
    background: url(../../Images/Kelloggs_Sprite.svg) no-repeat -309px -349px;
    width: 122px;
    height: 18px;
    content: "";
    display: inline-block;
    position: absolute;
    bottom: -1.4vw;
    transform: translateX(-100%);
}

.tbl_sampleSize tr.firstRow td:nth-child(2)::after {
    background: url(../../Images/Kelloggs_Sprite.svg) no-repeat -309px -349px;
    width: 122px;
    height: 18px;
    content: "";
    display: inline-block;
    position: absolute;
    bottom: -1.4vw;
    transform: translateX(-83%);
}

.tbl_sampleSize tr.firstRow td:last-child::after {
    background: url(../../Images/Kelloggs_Sprite.svg) no-repeat -309px -349px;
    width: 122px;
    height: 18px;
    content: "";
    display: inline-block;
    position: absolute;
    bottom: -1.4vw;
    transform: translateX(-95%);
}

