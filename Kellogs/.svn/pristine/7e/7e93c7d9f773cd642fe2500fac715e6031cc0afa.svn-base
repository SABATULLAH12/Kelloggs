.downloadLink {
    position: absolute;
    top: 0.5vw;
    right: -1%;
    zoom: 2;
}

.downloadImage {
    background: url(../Images/download-button.svg);
    background-size: contain;
    cursor: pointer;
    height: 2vw;
    width: 2vw;
}

    .downloadImage a {
        height: 2vw;
        width: 2vw;
        position: absolute;
        left: 0;
        cursor: pointer;
    }

#mediaVideo {
    height: 100%;
    width: 100%;
}

/*custom video styling start*/
.vjs-default-skin {
    position: relative !important;
    height: 100% !important;
    width: 100% !important;
}

    .vjs-default-skin.vjs-paused .vjs-play-control div {
        width: 15px;
        height: 17px;
        background: url('http://vjs.zencdn.net/c/video-js.png');
        margin: .5em auto 0;
    }

    .vjs-default-skin.vjs-playing .vjs-play-control div {
        width: 15px;
        height: 17px;
        background: url('http://vjs.zencdn.net/c/video-js.png') -25px 0;
        margin: .5em auto 0;
    }

    .vjs-default-skin .vjs-rewind-control div {
        width: 19px;
        height: 16px;
        background: url('http://vjs.zencdn.net/c/video-js.png');
        margin: .5em auto 0;
    }

    .vjs-default-skin .vjs-mute-control div {
        width: 22px;
        height: 16px;
        background: url('http://vjs.zencdn.net/c/video-js.png') -75px -25px;
        margin: .5em auto 0;
    }

    .vjs-default-skin .vjs-mute-control.vjs-vol-0 div {
        background: url('http://vjs.zencdn.net/c/video-js.png') 0 -25px;
    }

    .vjs-default-skin .vjs-mute-control.vjs-vol-1 div {
        background: url('http://vjs.zencdn.net/c/video-js.png') -25px -25px;
    }

    .vjs-default-skin .vjs-mute-control.vjs-vol-2 div {
        background: url('http://vjs.zencdn.net/c/video-js.png') -50px -25px;
    }

    .vjs-default-skin div.vjs-seek-handle {
        position: absolute;
        width: 16px;
        height: 16px;
        margin-top: -0.3em;
        left: 0;
        top: 0;
        background: url('http://vjs.zencdn.net/c/video-js.png') 0 -50px;
    }

    .vjs-default-skin .vjs-fullscreen-control div {
        width: 16px;
        height: 16px;
        background: url('http://vjs.zencdn.net/c/video-js.png') -50px 0;
        margin: .5em auto 0;
    }

    .vjs-default-skin.vjs-fullscreen .vjs-fullscreen-control div {
        background: url('http://vjs.zencdn.net/c/video-js.png') -75px 0;
    }

    .vjs-default-skin .vjs-big-play-button {
        display: block;
        z-index: 2;
        position: absolute;
        top: 50%;
        left: 50%;
        width: 5.0em;
        height: 3.0em;
        transform: translate(-50%,-50%);
        /*margin:-42px 0 0 -42px;*/
        text-align: center;
        vertical-align: middle;
        cursor: pointer !important;
        border: .2em solid #fff;
        background: #454545;
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#454545', endColorstr='#3f3f3f', GradientType=0);
    }

    .vjs-default-skin div.vjs-big-play-button span {
        position: absolute;
        top: 50%;
        left: 50%;
        display: block;
        width: 35px;
        height: 42px;
        margin: -20px 0 0 -15px;
        background: url('http://vjs.zencdn.net/c/video-js.png') -100px 0;
    }

    .vjs-default-skin .vjs-menu-button div {
        background: url('http://vjs.zencdn.net/c/video-js.png') 0 -75px no-repeat;
        width: 16px;
        height: 16px;
        margin: .2em auto 0;
        padding: 0;
    }

    .vjs-default-skin .vjs-menu-button ul li.vjs-selected {
        text-decoration: underline;
        background: url('http://vjs.zencdn.net/c/video-js.png') -125px -50px no-repeat;
    }

    .vjs-default-skin .vjs-big-play-button {
        position: absolute !important;
        top: 50% !important;
        left: 50% !important;
    }

    .vjs-default-skin .vjs-mute-control.vjs-vol-0:before, .vjs-default-skin .vjs-volume-menu-button.vjs-vol-0:before {
        content: none;
    }

    .vjs-default-skin .vjs-mute-control:before, .vjs-default-skin .vjs-volume-menu-button:before {
        content: none;
    }

    .vjs-default-skin.vjs-playing .vjs-play-control:before, .vjs-default-skin .vjs-play-control:before {
        content: none;
    }

    .vjs-default-skin.vjs-fullscreen .vjs-fullscreen-control:before, .vjs-default-skin.vjs-fullscreen .vjs-fullscreen-control:before, .vjs-default-skin .vjs-fullscreen-control:before {
        content: none;
    }

    .vjs-default-skin .vjs-big-play-button:before {
        content: none;
    }

/*custom video styling end*/

/* LOV Market & Time Period Popup start*/

crosstabTableDiv {
    height: 100%;
    width: 100%;
    float: left;
    position: relative;
}

.crosstabTable {
    height: 100%;
    width: 100%;
    float: left;
    position: relative;
}

.leftHeader {
    width: 22vw;
    height: auto;
    position: relative;
    float: left;
}

.rightHeader {
    width: calc(100% - 22vw);
    height: auto;
    position: relative;
    overflow: hidden;
    float: left;
}

.leftBody {
    width: 22vw;
    height: calc(100% - 3.6vw);
    position: relative;
    overflow: hidden;
    float: left;
}


.rightBody {
    width: calc(100% - 22vw);
    height: calc(100% - 3.6vw);
    position: relative;
    float: left;
    overflow: auto;
}

    .rightBody.base, .leftBody.base {
        height: calc(100% - 6.9vw);
    }

.row-item {
    clear: both;
    width: 100%;
    height: 3.3vw;
    color: #414042;
    font-size: calc(.6vw + .6vh + .3vmin);
    /*font-size: calc(.4vw + .5vh + .4vmin);*/
    text-transform: uppercase;
    color: #414042;
    font-weight: Bold;
    position: relative;
    overflow: hidden;
    border-bottom: 1px dashed #E0E0E0;
}

.leftBody .row-item, .rightBody .row-item {
    height: calc(100% / 8);
}

.row-item-div {
    position: relative;
    float: left;
    height: 100%;
    border: none;
    /*box-shadow: 5px 0 5px -5px inset #E0E0E0;*/
}


.row-item-head-content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: 1vw;
    width: 100%;
    text-align: center;
    display: -ms-inline-flexbox;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    text-transform: uppercase;
    padding: 0 0.37vw;
}

.row-left-item-content-title {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: calc(.6vw + .6vh + .4vmin);
    width: 100%;
    text-align: left;
    display: -ms-inline-flexbox;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    text-transform: uppercase;
    padding: 0 0.37vw;
    margin-left: 0.73vw;
    /*border-left: 2px solid #D21E47;*/
}

row-right-item-container {
    position: absolute;
    width: 95%;
    left: 50%;
    transform: translateX(-50%);
    height: 100%;
}

.row-right-item-content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: inherit;
    font-family: inherit;
    font-size: 1vw;
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    line-height: 1;
}

.leftBody .row-item .row-item-div:first-child .row-right-item-content, .leftHeader .row-item .row-item-div:first-child .row-item-head-content {
    /*border-left: 2px solid #D21E47;*/
    width: calc(100% - 1.47vw);
    margin-left: 0.73vw;
    padding: 0 0.73vw;
    text-align: left;
}

.row-item-value {
    padding: 0 0.73vw;
    width: 100%;
    float: left;
    text-align: center;
}

.leftBorder {
    position: absolute;
    height: 0.73vw;
    width: 0.15vw;
    background: #D21E47;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}

.TMPopup {
    height: 98% !important;
}

    .TMPopup .row-item {
        border: none;
    }

    .TMPopup .rightHeader .row-item {
        background-color: #D31245;
        color: white;
    }

    .TMPopup .row-item-div {
        border-bottom: 1px solid #E0E0E0;
        /*border-right: 1px solid #e0e0e0;*/
    }

    .TMPopup .leftBody .row-item-div {
        border-right: 1px solid #e0e0e0;
    }


    .TMPopup .row-item-head-content {
        position: relative;
        top: 0;
        left: 0;
        transform: none !important;
        border-right: 1px solid #e0e0e0;
    }

        .TMPopup .row-item-head-content.middleAlign {
            top: 50%;
            transform: translateY(-50%) !important;
        }

    .TMPopup .head-div {
        transform: none;
        float: left;
        position: relative;
        font-size: 0.9vw;
        /*margin-top: 4%;*/
        top: 0;
        left: 0;
        height: 100%;
    }

    .TMPopup .item-container {
        height: 100%;
        float: left;
        position: relative;
    }

    .TMPopup .tickImg {
        height: 60%;
        width: 100%;
    }

        .TMPopup .tickImg img {
            width: 0.9vw;
        }

    .TMPopup .leftBody, .TMPopup .rightBody {
        height: calc(100% - 6vw);
    }

