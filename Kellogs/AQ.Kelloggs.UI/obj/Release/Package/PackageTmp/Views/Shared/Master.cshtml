    <link href="~/Content/video-js.swf" />
    @*video js plugin end*@
    <link href="~/Content/Fonts/CustomFonts.css" rel="stylesheet" />
    <script src="~/Scripts/Common/HelperFunction.js"></script>

    <script src="~/Scripts/Common/App.js"></script>

    <script type="text/javascript">

        function preventBack() { window.history.forward(); }
        setTimeout("preventBack()", 0);
        window.onunload = function () { null };

        function myFunction() {
            document.getElementById("myDropdown").classList.toggle("show");
            //    document.getElementById('mySettings').style.background = "none";
            //     document.getElementById('mySettings').style.backgroundColor = "white";

        }

        function myFunction1() {
            document.getElementById("myDropdown").classList.toggle("show");
            document.getElementById('mySettings').style.background = "";
            document.getElementById('mySettings').style.backgroundColor = "";

        }

        // Close the dropdown menu if the user clicks outside of it
        window.onclick = function (event) {
            if (!angular.element(event.target).hasClass('dropbtn')) {
                var dropdowns = document.getElementsByClassName("settingDropdown");
                var i;
                for (i = 0; i < dropdowns.length; i++) {
                    var openDropdown = dropdowns[i];
                    if (openDropdown.classList.contains('show')) {
                        openDropdown.classList.remove('show');
                    }
                }
                document.getElementById('mySettings').style.background = "";
                document.getElementById('mySettings').style.backgroundColor = "";
            }

            if (!angular.element(event.target).hasClass('trendChartIcon'))
                $("#trendchartCallout").hide();
            if (!angular.element(event.target).hasClass('stackChartIcon'))
                $("#chartCallout").hide();
            if (!angular.element(event.target).hasClass('retailText') && !angular.element(event.target).hasClass('retailIconImage') && !angular.element(event.target).hasClass('retailValue'))
                $("#retailCallout").hide();

        }
        sessionStorage.setItem("Username", "@Session["Username"].ToString()");
        sessionStorage.setItem("Roles", "@Session["Roles"].ToString()");
        sessionStorage.setItem("Email", "@Session["Email"].ToString()");
        sessionStorage.setItem("UserId", "@Session["UserId"].ToString()");
        sessionStorage.setItem("Modules", "@Session["Modules"].ToString()");

        var outputNeededStickySelection = '@System.Configuration.ConfigurationManager.AppSettings["IsSSOutputNeeded"].ToString()' == "true" ? true : false;
        var version = '@System.Configuration.ConfigurationManager.AppSettings["Version"].ToString()';
        document.write('<link href="../Content/Shared/Master.css?version=' + version + '" rel="stylesheet" type="text/css" />');
        document.write('<script src="../Scripts/Common/Constans.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
        document.write('<script src="../Scripts/Common/Common.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
        document.write('<script src="../Scripts/Common/Services.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
        if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
            document.write('<script src="../Scripts/Plugins/Html2Canvas/promise.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
            document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvasIE.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
            document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvasIE.svg.min.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
            //document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvasIE.min.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
        }
        else {
            document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvas.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
            document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvas.min.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
            document.write('<script src="../Scripts/Plugins/Html2Canvas/html2canvas.svg.min.js?version=' + version + '" type="text/javascript"></scr' + 'ipt>');
        }
    </script>
</head>
<body id="parentBody" ng-app="starter" ng-controller="parent-controller">
    <content id="parent">
        <div class="default-background_landing" style="display:none"></div>
        <div class="overlay1"></div>
        <div class="overlay"></div>
        @*background default start*@
        <div class="default-background"></div>
        @*background default end*@
        @*heade start*@
        <div class="body-header  ng-cloak">
            <div class="body-header-menus">
                <div class="header-left">
                    <div class="header-arrow-styles" ng-click="ModulePanelToggle()">
                        <div class="middleAlign">
                            <div class="header-arrow"></div>
                        </div>
                    </div>
                    <div class="module-name">
                        <div class="middleAlign">
                            Visual CrossTab
                        </div>
                    </div>
                </div>
                <div class="header-center">

                </div>
                <div class="header-right">

                    <div class="header-logo-styles">
                        <div class="header-logo"></div>
                    </div>

                    <div class="icons">
                        <div class="home" onclick="location.href='@Url.Action("Home", "Home")'"></div>
                    </div>
                    @*settings dropdwon div - start*@
                    <div class="settings dropdown">
                        <button onclick="myFunction()" class="dropbtn" id="mySettings"></button>
                        <div id="myDropdown" class="settingDropdown">
                            <div class="settingsDiv ddOptions">
                                <div class="middleAlign">
                                    <div class="settings active" onclick="myFunction1()"></div>
                                </div>
                            </div>
                            <div class="excelDiv ddOptions disableSelection" ng-click="Exports('excel')" title="Export to Excel">
                                <div class="middleAlign">
                                    <div class="exclIcon ddIcon ddExclIcon"></div>
                                </div>
                            </div>
                            <div class="pptDiv ddOptions disableSelection" ng-click="Exports('ppt')" title="Export to PPT">
                                <div class="middleAlign">
                                    <div class="pptIcon ddIcon ddPptIcon"></div>
                                </div>
                            </div>
                            <div class="helpDiv ddOptions " ng-click="downloadUserGuide(1,'abc')" title="Download User Guide" style="box-shadow:none">
                                <div class="middleAlign">
                                    <div class="helpIcon ddHIcon ddHelpIcon"></div>
                                </div>
                            </div>
                            <div class="logoutDiv ddOptions disableSelection" title="Logout" style="display:none">
                                <div class="middleAlign">
                                    <div class="logoutIcon ddIcon ddLogoutIcon" onclick="location.href='@Url.Action("Login", "Login")'"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*settings dropdwon div - end*@
                </div>
            </div>

            <div class="body-header-filterSummery">

                <div class="summary-content">
                    <div class="summaryName">
                        <div class="summarytitle">
                            <div class="middleAlign">Selections:</div>
                        </div>
                    </div>
                    <div class="summay-content-text">
                        <div class="middleAlign"></div>
                    </div>

                    <div class="summary-view">
                        <div class="summary-view-details">
                            <div class="summary-view-click" ng-click="showAllSummary()"></div>
                        </div>
                    </div>
                </div>
                @*<div class="save_filter"  title="Save Filter">
                        <div class="saveFilterIconDiv">
                            <div class="middleAlign"><div class="saveFilterIcon"></div></div>
                        </div>
                        <div class="saveFilterText"><div class="middleAlign">SAVE FILTER</div></div>
                    </div>*@
                <div class="save_filter_icon disableSelection" title="Save Filter" onclick="custom_filters()">
                    <div class="middleAlign">
                        <div class="saveFilterIcon"></div>
                    </div>
                </div>
                <div class="left-panel-view" id="left_panel_click_element">
                    <div class="panel-left-div">
                        <div class="panel-click middleAlign" ng-click="leftPanelToggle($event)"></div>
                    </div>
                </div>
            </div>

        </div>
        @*header end*@

        @*body start*@
        <div class="body-container ng-cloak">

            <div class="modulePanel ng-cloak" ng-hide="ModuleIsHidden">
                <img src="../Content/Images/Kelloggs_Menu bg thick.jpg" style="position: absolute;height: 100%;width :100%">
                <div class="moduleLevel"></div>
                <div class="moduleLevel" ng-class="[{'disableSelection' :!checkUserAccess('2','Occasion Profile')}]" ng-click="loadModule(event,'../Snapshot/Snapshot?moduleid=2')" title="OCCASION PROFILE">
