        @*custom popup start*@
        <div class="custom-popup">
            <div class="popup-header">
                <div class="head-warnong-icon"><div class="headLeftBorder"></div></div>
                <div class="head-msg" id="custom_message">
                    <div class="middleAlign"> ALERT </div>
                </div>
                <div class="head-close-icon" ng-click="close_Custom_popup()"></div>
                <div class="bottomShadow" style="bottom: -0.2vw;background: white;"><img no-repeat="" src="../Content/Images/Kelloggs_horizontal_shadow.png"></div>
            </div>
            <div class="popup-container">
                <div class="middleAlign">content</div>
            </div>

        </div>
        @*custom popup end*@

        @*custom popup start*@
        <div class="samplesize-popup">
            <div class="popup-header">
                <div class="head-warnong-icon"><div class="headLeftBorder"></div></div>
                <div class="head-msg samplesizeHead" id="custom_message">
                    <div class="middleAlign">SAMPLE SIZE</div>
                </div>
                <div class="weightedOccasionDiv sampleTypeDiv" style="display:none" onclick="selectSampleType(event)">
                    <div class="weightedIconDiv"><div class="middleAlign"><div class="sampleTypeIcon"></div></div></div>
                    <div class="weightedTextDiv"><div class="middleAlign">Kellogg Weighted Occasion</div></div>
                </div>
                <div class="unweightedOccasionDiv sampleTypeDiv" style="display:none;box-shadow: 0.37vw 0 0.37vw -0.3vw #333;" onclick="selectSampleType(event)">
                    <div class="unweightedIconDiv"><div class="middleAlign"><div class="sampleTypeIcon"></div></div></div>
                    <div class="unweightedTextDiv"><div class="middleAlign">Kellogg Unweighted Occasion</div></div>
                </div>
                <div class="weightedOccasionDiv sampleTypeDiv" onclick="selectSampleType(event)">
                    <div class="weightedIconDiv"><div class="middleAlign"><div class="sampleTypeIcon"></div></div></div>
                    <div class="weightedTextDiv"><div class="middleAlign">Weighted Occasion</div></div>
                </div>
                <div class="unweightedOccasionDiv sampleTypeDiv" onclick="selectSampleType(event)">
                    <div class="unweightedIconDiv"><div class="middleAlign"><div class="sampleTypeIcon"></div></div></div>
                    <div class="unweightedTextDiv"><div class="middleAlign">Unweighted Occasion</div></div>
                </div>
                <div class="head-close-icon" ng-click="close_Custom_popup()"></div>
                <div class="bottomShadow"><img no-repeat="" src="../Content/Images/Kelloggs_horizontal_shadow.png"></div>
            </div>
            <div class="popup-container" id="samplesize_content">

            </div>
        </div>
        @*custom popup end*@

        @*custom popup for change password start*@
        <div class="changePwdBox" ng-class="showchangePwdBox ? 'changePwdBlock' : 'changePwdNone'">
            <div class="changePwdTopBox">
                <div class="changePwdCross" ng-click="closechangePwdBox()"></div>
            </div>
            <div class="changePwdFormBox" ng-class="toggleFormResetBox ? 'changePwdNone' : 'changePwdBlock'">
                <div class="changePwdTitleBox">
                    <div class="changePwdTitle middleAlign">RESET YOUR PASSWORD</div>
                </div>
                <div class="changePwdText">
                    <div class="middleAlign">Please enter password</div>
                </div>
                <div class="changePwdContainer">
                    <input class="changePwdTextBox oldPwd" type="password" placeholder="ENTER CURRENT PASSWORD" id="crntPass" ng-model="crntPass" ng-style="curPwdstyl" ng-focus="inputBox()" autocomplete="off" />
                </div>
                <div class="changePwdContainer">
                    <input class="changePwdTextBox newPwd" type="password" placeholder="ENTER NEW PASSWORD" id="newPass" ng-model="newPass" ng-style="newPwdstyl" ng-focus="inputBox()" autocomplete="off" />
                </div>
                <div class="changePwdContainer">
                    <input class="changePwdTextBox newPwd" type="password" placeholder="RE-ENTER NEW PASSWORD" id="reNewPass" ng-model="reNewPass" ng-style="newPwdstyl" ng-focus="inputBox()" autocomplete="off" />
                </div>
                <div class="changePwdBottomBox">
                    <div class="changePwdInvalidPass">
                        <div class="changePwdInvalidPassText middleAlign" ng-model="changePwdInvalidPassText"
                             ng-class="showInvalidPwdText ? 'changePwdBlock' : 'changePwdNone'"></div>
                    </div>
                    <div class="changePwdSubmitImg" ng-click="changePassword(crntPass, newPass, reNewPass)">
                        <div class="changePwdSubmitText">SUBMIT</div>
                    </div>
                </div>
            </div>
            <div class="changePwdResetBox" ng-class="toggleFormResetBox ? 'changePwdBlock' : 'changePwdNone'"></div>
        </div>
        @*custom popup for change password start*@

        @*custom loader icon start*@
        <div class="default-loader-icon"></div>
        @*custom loader icon end*@

        @*show summary popup start*@
        <div class="summaryPopup">
            <div class="headerSummary"><span>Selections :</span></div>
            <div class="pop_selection_summ"></div>
            <div class="summary-view">
                <div class="summary-view-details">
                    <div class="summary-view-click" ng-click="showAllSummary()"></div>
                </div>
            </div>
        </div>
        @*show summary popup end*@

        @*translucent back div in common - start*@
        <div class="translucentDiv" ng-class="{'show' : customPopup['visible'] == true}"></div>
        @*translucent back div in common - end*@

        <div class="alert-overlay"></div>
        <div class="alert-popup">
            <div class="popup-header">
                <div class="head-warnong-icon"><div class="headLeftBorder"></div></div>
                <div class="head-msg" id="custom_message">
                    <div class="middleAlign"> ALERT </div>
                </div>
                <div class="head-close-icon" onclick="close_alert_popup()"></div>
            </div>
            <div class="popup-container">
                <div class="middleAlign">content</div>
            </div>

        </div>



    </content>
    <div id="warning-message">
        this website is only viewable in landscape mode
    </div>
</body>

</html>
