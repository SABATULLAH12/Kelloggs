                        <div class="middleAlign">
                            <div class="moduleLevel1">
                            </div>
                        </div>
                    </div>
                    <div class="menu-tab-center">
                        <div class="menu-tab-name">
                            <div class="middleAlign">OCCASION PROFILE</div>
                        </div>
                    </div>

                </div>
                <div class="moduleLevel" ng-class="[{'disableSelection' :!checkUserAccess('9','Performance Dashboard')}]" ng-click="loadModule(event,'../PerformanceDashboard/PerformanceDashboard?moduleid=9')" title="PERFORMANCE DASHBOARD">
                    <div class="menu-tab-left">
                        <div class="middleAlign">
                            <div class="moduleLevel6">
                            </div>
                        </div>
                    </div>
                    <div class="menu-tab-center">
                        <div class="menu-tab-name">
                            <div class="middleAlign">PERFORMANCE DASHBOARD</div>
                        </div>
                    </div>

                </div>
                <div class="moduleLevel" ng-class="[{'disableSelection' :!checkUserAccess('2','Visual Crosstab')}]" ng-click="loadModule(event,'../Crosstab/Crosstab?moduleid=3')" title="VISUAL CROSSTAB">
                    <div class="menu-tab-left">
                        <div class="middleAlign">
                            <div class="moduleLevel2">
                            </div>
                        </div>
                    </div>
                    <div class="menu-tab-center">
                        <div class="menu-tab-name">
                            <div class="middleAlign">VISUAL CROSSTAB</div>
                        </div>
                    </div>
                </div>
                <div class="moduleLevel" ng-class="[{'disableSelection' :!checkUserAccess('0','Advanced Analytics')}]" ng-click="showOptions()" title="ADVANCED ANALYTICS">
                    <div class="menu-tab-left">
                        <div class="middleAlign">
                            <div class="moduleLevel3">
                            </div>
                        </div>
                    </div>
                    <div class="menu-tab-center">
                        <div class="menu-tab-name">
                            <div class="middleAlign">ADVANCED ANALYTICS</div>
                        </div>
                    </div>
                </div>
                <div class="moduleLevel" ng-show="!IsOptionsHidden" ng-style="!IsOptionsHidden ? {'background':'#222222'} : ''">

                    <div class="moduleLevel" ng-class="[{'disableSelection' :!checkUserAccess('4','Occasion Strategic Postures')}]" ng-click="loadModule(event,'../AdvancedAnalytics/Osp?moduleid=4')" style="height:50%;" title="STRATEGIC POSTURES">
