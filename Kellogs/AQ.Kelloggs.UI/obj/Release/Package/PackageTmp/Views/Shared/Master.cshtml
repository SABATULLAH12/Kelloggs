    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/Plugins/d3/d3.min.js"></script>
    @*jquery plugin end*@

    @*Angular Js plugin - start*@
    <script src="~/Scripts/Plugins/angular1.5.8/angular.js"></script>
    <script src="~/Scripts/Plugins/angular1.5.8/angular-animate.js"></script>
    <script src="~/Scripts/Plugins/angular1.5.8/angular-route.js"></script>
    <script src="~/Scripts/Plugins/angular-recursion.min.js"></script>
    <script src="~/Scripts/Plugins/niceScroll.js"></script>
    @*Angular Js plugin - end*@
    @*Html2Canvas start*@
    <script src="~/Scripts/Plugins/Html2Canvas/canvg.min.js"></script>
    <script src="~/Scripts/Plugins/Html2Canvas/rgbcolor.js"></script>


    @*Html2Canvas start*@
    @*plugin for search dropdown - start*@
    <script src="~/Scripts/Plugins/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Scripts/Plugins/bootstrap.min.js"></script>
